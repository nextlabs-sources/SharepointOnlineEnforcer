 <Obligations>
    <Obligation>
        <DisplayName>SP Trimming condition</DisplayName>
        <RunAt>PEP</RunAt>
        <Name>SP_Trimming_condition</Name>
        <Arguments> 
            <Argument usereditable="true"> 
                <Name>Condition Name</Name>
                <Value>URL</Value>
                <Value>CREATEBY</Value> 
            </Argument>
            <Argument usereditable="false">
                <Name>operator</Name>
                <Value>Equals</Value>
                <Value>Inequality</Value>
                <Value>Include</Value>
                <Value>Exclude</Value>
                <Value>Bigger</Value>
                <Value>Smaller</Value>
            </Argument>
            <Argument usereditable="true"> 
                <Name>Condition Value</Name> 
            </Argument>
            <Argument usereditable="false"> 
                <Name>Enforce</Name>
                <Value default="true">Allow</Value>
                <Value default="true">Deny</Value> 
            </Argument>
        </Arguments>
    </Obligation>
    
    <Obligation>
        <DisplayName>SharePoint Online User-Defined Tagging</DisplayName>
        <RunAt>PEP</RunAt>
        <Name>SP_File_Tag</Name>
        <Arguments>
            <Argument usereditable="true"> 
            <Name>Tag Name</Name>
        </Argument> 
        <Argument usereditable="true">
            <Name>Tag Value</Name>
        </Argument>
      </Arguments>
    </Obligation>

    <Obligation>
        <DisplayName>Set Column Value</DisplayName>
        <RunAt>PEP</RunAt>
        <Name>SET_COLUMN_VALUE</Name>
        <Arguments>  
            <Argument usereditable="true"> 
                <Name>Column Name</Name>
            </Argument> 
            <Argument usereditable="true">
                <Name>Column Value</Name>
            </Argument>
        </Arguments>
    </Obligation>

    <Obligation>
        <DisplayName>SharePoint Online Rights Management</DisplayName>
        <RunAt>PEP</RunAt>
        <Name>SPOE_Upload_Encrypt</Name>
    </Obligation>
</Obligations>

<ActionList>
    <Action> 
      <Name>UPLOAD</Name> 
      <DisplayName>Upload</DisplayName> 
      <ShortName>UP</ShortName> 
      <Category>Transform</Category>
    </Action>
</ActionList>

